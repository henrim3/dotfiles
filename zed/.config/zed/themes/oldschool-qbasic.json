// https://github.com/EricsonWillians/oldschool-theme
{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Oldschool QBasic",
  "author": "Ericson Williams",
  "themes": [
    {
      "name": "Oldschool QBasic",
      "appearance": "dark",
      "style": {
        "background": "#000050ff",
        "surface.background": "#000000ff",
        "elevated_surface.background": "#000000ff",

        "border": "#ffffff88",
        "border.variant": "#ffffffff",
        "border.focused": "#ffffffff",
        "border.selected": "#ffffffff",
        "border.disabled": "#ffffffff",
        "border.transparent": "#00000000",

        "text": "#ffffffff",
        "text.muted": "#cfcfc2ff",
        "text.disabled": "#8f8f8fff",
        "text.placeholder": "#8f8f8fff",
        "text.accent": "#66d9efff",

        "icon": "#ffffffff",
        "icon.muted": "#cfcfc2ff",
        "icon.disabled": "#8f8f8fff",
        "icon.accent": "#66d9efff",

        "element.background": "#353535ff",
        "element.hover": "#444444ff",
        "element.active": "#555555ff",
        "element.selected": "#353535ff",
        "element.disabled": "#353535ff",

        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#444444ff",
        "ghost_element.active": "#555555ff",
        "ghost_element.selected": "#353535ff",
        "ghost_element.disabled": "#353535ff",

        "tab_bar.background": "#353535ff",
        "tab.inactive_background": "#353535ff",
        "tab.active_background": "#007070ff",

        "status_bar.background": "#353535ff",
        "title_bar.background": "#353535ff",
        "toolbar.background": "#353535ff",
        "panel.background": "#000000ff",

        "editor.background": "#000050ff",
        "editor.foreground": "#ffffffff",
        "editor.gutter.background": "#000050ff",
        "editor.active_line.background": "#353535aa",
        "editor.highlighted_line.background": "#353535ff",
        "editor.line_number": "#8f8f8fff",
        "editor.active_line_number": "#ffffffff",
        "editor.invisible": "#3b3a32ff",
        "editor.wrap_guide": "#ffffff22",
        "editor.active_wrap_guide": "#ffffff44",

        "editor.document_highlight.read_background": "#353535ff",
        "editor.document_highlight.write_background": "#353535ff",

        "scrollbar.thumb.background": "#ffffff88",
        "scrollbar.thumb.hover_background": "#ffffffaa",
        "scrollbar.track.background": "#353535ff",

        "terminal.background": "#000000ff",
        "terminal.foreground": "#ffffffff",

        "terminal.ansi.black": "#000000ff",
        "terminal.ansi.red": "#f92672ff",
        "terminal.ansi.green": "#a6e22eff",
        "terminal.ansi.yellow": "#e6db74ff",
        "terminal.ansi.blue": "#66d9efff",
        "terminal.ansi.magenta": "#ae81ffff",
        "terminal.ansi.cyan": "#66d9efff",
        "terminal.ansi.white": "#ffffffff",

        "error": "#f92672ff",
        "warning": "#e6db74ff",
        "info": "#66d9efff",
        "success": "#a6e22eff",

        "players": [
          {
            "cursor": "#f8f8f0ff",
            "background": "#f8f8f0ff",
            "selection": "#353535ff",
          },
        ],

        "syntax": {
          "comment": {
            "color": "#fd971fff",
          },
          "string": {
            "color": "#e6db74ff",
          },
          "number": {
            "color": "#ae81ffff",
          },
          "boolean": {
            "color": "#ae81ffff",
          },
          "constant": {
            "color": "#ae81ffff",
          },
          "keyword": {
            "color": "#f92672ff",
            "font_weight": 700,
          },
          "type": {
            "color": "#66d9efff",
            "font_style": "italic",
          },
          "function": {
            "color": "#a6e22eff",
          },
          "variable": {
            "color": "#ffffffff",
          },
          "property": {
            "color": "#a6e22eff",
          },
          "tag": {
            "color": "#f92672ff",
            "font_weight": 700,
          },
          "attribute": {
            "color": "#a6e22eff",
          },
          "namespace": {
            "color": "#66d9efff",
            "font_style": "italic",
          },
          "operator": {
            "color": "#ffffffff",
          },
          "punctuation": {
            "color": "#ffffffff",
          },
          "invalid": {
            "color": "#f8f8f0ff",
            "background": "#f92672ff",
            "font_weight": 700,
          },
          "markup.bold": {
            "color": "#ffffffff",
            "font_weight": 700,
          },
          "markup.italic": {
            "color": "#ffffffff",
            "font_style": "italic",
          },
          "markup.raw": {
            "color": "#66d9efff",
          },
          "title": {
            "color": "#a6e22eff",
            "font_weight": 700,
          },
        },
      },
    },
  ],
}
